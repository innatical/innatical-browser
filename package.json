{
  "name": "skye",
  "version": "6.6.4",
  "sideEffects": false,
  "description": "Extensible, fast and innovative web browser with Innatical UI.",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/skyebrowser/skye#readme",
  "main": "build/main.bundle.js",
  "author": "Innatical LLC <contact@innatical.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyebrowser/skye.git"
  },
  "bugs": {
    "url": "https://github.com/skyebrowser/skye/issues"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@cliqz/adblocker-electron": "1.26.6",
    "@electron/remote": "^2.0.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/animejs": "^3.1.6",
    "@types/chrome": "0.0.242",
    "@types/crypto-js": "^4.1.1",
    "@types/jszip": "^3.4.1",
    "@types/node": "20.4.5",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@wexond/rpc-core": "^1.0.3",
    "@wexond/rpc-electron": "^1.0.3",
    "animejs": "^3.2.1",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "electron": "^25.3.2",
    "electron-builder": "24.6.3",
    "electron-updater": "6.1.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "file-loader": "^6.2.0",
    "file-type": "18.5.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "icojs": "^0.17.1",
    "jszip": "^3.10.1",
    "mobx": "6.10.0",
    "mobx-react-lite": "4.0.3",
    "node-bookmarks-parser": "^2.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "3.0.0",
    "pretty-bytes": "6.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-windows-controls": "1.1.1",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "styled-components": "^6.0.5",
    "terser": "^5.19.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.6",
